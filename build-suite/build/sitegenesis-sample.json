{
  "dependencies": [
    {
      "repository": {
        "url": "git@bitbucket.org:demandware/sitegenesis.git/",
        "type": "git"
      },
      "source": {
        "path": ".",
        "glob": "**/*"
      },
      "siteImport": {
        "enabled": "true",
        "demoPath": "demo_data_no_hires_images"
      },
