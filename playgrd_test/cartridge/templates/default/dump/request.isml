<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">


<isset name="headerKeys" value="${pdict.request.httpHeaders.keySet().toArray()}" scope="page"/>
<isset name="headerMap"  value="${pdict.request.httpHeaders}" scope="page"/>



<div class="container">
    <h1>Request Header</h1>
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">Key</th>
                <th scope="col">Value</th>
            </tr>
        </thead>
        <tbody>

            <isloop items="${headerKeys}" var="headerKey">
                <tr>
                    <th scope="row">
                        <isprint value="${headerKey}" encoding="on" />
                    </th>
                    <td>
                        <isprint value="${headerMap.get(headerKey)}" encoding="on" />
                    </td>
                </tr>
            </isloop>
        </tbody>
    </table>

</div>



